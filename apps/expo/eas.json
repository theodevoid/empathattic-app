{
  "cli": {
    "version": ">= 3.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.15.0",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "preview"
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
